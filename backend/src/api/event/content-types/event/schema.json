{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime"
    },
    "description": {
      "type": "richtext"
    },
    "signupLink": {
      "type": "string",
      "regex": "[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "location": {
      "type": "string"
    },
    "visible": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
